		<outline created="Tue, 18 Jun 2013 15:06:28 GMT" text="Jeffrey Kishner" type="profile" url="http://jeff.smallpict.com/">
			<outline created="Tue, 18 Jun 2013 15:02:04 GMT" text="How to publish a quiz in Fargo 0.80">
				<outline created="Tue, 18 Jun 2013 15:02:22 GMT" text="Follow the directions in the &lt;a href=&quot;http://worknotes.smallpicture.com/june2013/fargo080&quot;&gt;worknotes&lt;/a&gt; to publish an outline"></outline>
				<outline created="Tue, 18 Jun 2013 15:03:01 GMT" text="To publish a quiz so that the answers are hidden (like &lt;a href=&quot;http://jeff.smallpict.com/xmlQuiz&quot;&gt;here&lt;/a&gt;), copy the following, paste it into your named outline, and change anything but the rules:">
					<outline created="Tue, 18 Jun 2013 14:11:33 GMT" text="Quiz" type="outline">
						<outline created="Tue, 18 Jun 2013 14:11:37 GMT" text="&lt;rules&gt;">
							<outline created="Tue, 18 Jun 2013 14:11:42 GMT" text="&lt;rule from=&quot;1&quot; to=&quot;infinity&quot;&gt;">
								<outline created="Tue, 18 Jun 2013 14:11:59 GMT" text="&lt;expanded&gt;false&lt;/expanded&gt;"></outline>
								<outline created="Tue, 18 Jun 2013 14:12:07 GMT" text="&lt;/rule&gt;"></outline>
								</outline>
							<outline created="Tue, 18 Jun 2013 14:12:08 GMT" text="&lt;/rules&gt;"></outline>
							</outline>
						<outline created="Tue, 18 Jun 2013 14:12:12 GMT" text="question 1">
							<outline created="Tue, 18 Jun 2013 14:12:52 GMT" text="answer 1"></outline>
							</outline>
						<outline created="Tue, 18 Jun 2013 14:12:53 GMT" text="question 2">
							<outline created="Tue, 18 Jun 2013 14:13:21 GMT" text="answer 2"></outline>
							</outline>
						</outline>
					</outline>
				</outline>
			</outline>
